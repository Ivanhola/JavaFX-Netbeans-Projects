<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="260.0" prefWidth="357.0" stylesheets="@../../../resources/general.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="library.assistant.settings.SettingsController">
   <children>
      <VBox layoutY="6.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="numDaysNoFine" promptText="Number of days member can take book without fine">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="finePerDay" promptText="Fine per day">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="username" promptText="Username">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="password" promptText="password">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </PasswordField>
         </children>
      </VBox>
      <HBox layoutY="201.0" prefHeight="43.0" prefWidth="357.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <JFXButton onAction="#handleSaveButtonAction" prefHeight="100.0" prefWidth="200.0" text="Save" />
            <JFXButton layoutX="10.0" layoutY="10.0" onAction="#handleCancelButtonAction" prefHeight="100.0" prefWidth="200.0" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
